    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
